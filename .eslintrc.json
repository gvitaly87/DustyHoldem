{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier", "ejs"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "settings": {
      "import/extensions": [".mjs", ".js"],
      "import/resolver": {
        "node": {
          "extensions": [".mjs", ".js"]
        }
      }
    }
  }
}
